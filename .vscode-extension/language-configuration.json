{
  "comments": {
    "lineComment": "#",
    "blockComment": ["(", ")"]
  },
  "brackets": [
    ["[", "]"],
    ["{", "}"],
    [":", ";"]
  ],
  "autoClosingPairs": [
    ["[", "]"],
    ["{", "}"],
    ["\"", "\""],
    [":", ";"]
  ],
  "surroundingPairs": [
    ["[", "]"],
    ["{", "}"],
    ["\"", "\""]
  ],
  "indentationRules": {
    "increaseIndentPattern": "^\\s*:",
    "decreaseIndentPattern": "^\\s*;"
  }
}